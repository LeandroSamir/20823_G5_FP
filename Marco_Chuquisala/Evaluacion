#include <stdio.h>
int main() {
    int vector[10];
    int i, norma;
    norma=0;
    printf("Ingrese lo componentes:\n");
    for (int i = 1; i < 10; i++) {
        printf("Componente %d: ", i);
        scanf("%d", &vector[i]);
        norma=norma+vector[i] * vector[i];
    }
    printf("La norma de los componentes es: %d\n", norma);
    return 0;
}
