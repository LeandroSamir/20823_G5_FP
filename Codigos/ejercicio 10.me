#include <stdio.h>
#include "ejercicios.h"

void ejercicio10() {
	
int es_primo(int n) {
    if (n <= 1) return 0;
    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0) return 0;
    }
}


    int a, b;

    printf("Ingrese el primer numero: ");
    scanf("%d", &a);
    printf("Ingrese el segundo numero: ");
    scanf("%d", &b);

    for (int i = a; i <= b; i++) {
        if (es_primo(i)) {
            printf("%d ", i);
        }
    }

    printf("\n");
}
